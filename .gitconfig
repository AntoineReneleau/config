[user]
  name = Antoine RENELEAU
	email = antoine@reneleau.fr

[alias]
  st = status
  df = diff
  co = checkout
  ci = commit
  br = branch

[push]
	default = simple

[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
    whitespace = red reverse
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan
  [core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
